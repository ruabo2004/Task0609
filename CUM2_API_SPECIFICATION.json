    "folder_structure": {
      "components/Room/": [
        "RoomCard.js",
        "RoomGrid.js",
        "RoomList.js",
        "RoomFilter.js",
        "RoomSort.js",
        "RoomImageGallery.js",
        "RoomAmenities.js",
        "PriceDisplay.js"
      ],
      "components/Search/": [
        "SearchForm.js",
        "DateRangePicker.js",
        "GuestSelector.js",
        "QuickFilters.js"
      ],
      "pages/": [
        "Rooms.js",
        "RoomDetail.js",
        "SearchResults.js"
      ],
      "hooks/": [
        "useRooms.js",
        "useRoomTypes.js",
        "useRoomSearch.js",
        "useRoomFilters.js"
      ],
      "services/": [
        "roomService.js"
      ],
      "utils/": [
        "roomHelpers.js",
        "priceCalculator.js",
        "dateHelpers.js"
      ]
    },

    "key_features": [
      "Room grid and list view layouts",
      "Advanced search and filtering",
      "Date range picker integration",
      "Real-time availability checking",
      "Price calculation with weekend/holiday rates",
      "Image gallery with lazy loading",
      "Responsive design for all devices",
      "URL-based filter persistence",
      "Infinite scroll pagination",
      "Sorting and view preferences"
    ],

